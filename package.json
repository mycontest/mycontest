{
  "name": "mycontest",
  "version": "2.0.0",
  "description": "mycontest",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --ignore session/ ./src/server.js",
    "db:init": "mysql -u root -p < ./database/init.sql",
    "db:seed": "mysql -u root -p < ./database/seed.sql",
    "db:reset": "npm run db:init && npm run db:seed",
    "backup": "node scripts/backup.js",
    "backup:db": "mysqldump -u root -p my_contest > ../data/backup_$(date +%Y%m%d_%H%M%S).sql",
    "backup:full": "tar -czf ../mycontest_backup_$(date +%Y%m%d_%H%M%S).tar.gz ../ --exclude=node_modules --exclude=.git --exclude=session --exclude=temp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mycontest/mycontest"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mycontest/mycontest/issues"
  },
  "homepage": "https://github.com/mycontest/mycontest#readme",
  "dependencies": {
    "adm-zip": "^0.5.16",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-fileupload": "^1.3.1",
    "express-session": "^1.17.2",
    "generate-password": "^1.7.0",
    "generate-sms-verification-code": "^1.0.5",
    "glob": "^11.0.0",
    "google-recaptcha": "^1.1.0",
    "joi": "^17.6.0",
    "md5": "^2.3.0",
    "multer": "^2.0.2",
    "mysql2": "^3.16.0",
    "node-telegram-bot-api": "^0.56.0",
    "session-file-store": "^1.5.0",
    "uid-generator": "^2.0.0",
    "unzipper": "^0.12.3",
    "uzdev": "^1.6.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}
