<script>
    function display() {
        let a = (e) => {
            if (e < 10) return "0" + e;
            else return e;
        };
        let today = new Date();
        let month = today.getMonth();
        let day = today.getDate();
        let year = today.getFullYear();
        let hour = today.getHours();
        let minute = today.getMinutes();
        let second = today.getSeconds();
        let output = a(year) + "-" + a(month + 1) + "-" + a(day) + " " + a(hour) + ":" + a(minute) + ":" + a(second);
        if (document.getElementById("current_date")) document.getElementById("current_date").innerText = output;
    }
    try {
        display();
    } catch {
        console.log("Hi");
    }
    setInterval(display, 500);
</script>

